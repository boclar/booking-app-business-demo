        "commit": "cz",
        "devtools": "react-devtools",
        "eslint": "eslint src/**/*.{ts,tsx}",
        "eslint:fix": "eslint src/**/*.{ts,tsx} --fix",
        "generate:component": "plop generate:component",
        "generate:context": "plop generate:singular-generic",
        "generate:hook": "plop generate:plural-generic",
        "generate:screen": "plop generate:screen",
        "generate:util": "plop generate:plural-generic",
        "ios": "expo run:ios",
        "lint": "yarn eslint && yarn prettier",
        "lint:fix": "concurrently \"yarn eslint:fix\" \"yarn prettier:fix\"",
        "locize:download": "locize download --api-key bc1331c5-41e6-4874-b0e7-27a86e77dd1b --project-id b5633159-e369-4975-95e2-d1a7d99420ff --ver latest --path ./src/locales",
        "locize:sync": "locize sync --api-key bc1331c5-41e6-4874-b0e7-27a86e77dd1b --project-id b5633159-e369-4975-95e2-d1a7d99420ff --ver latest --path ./src/locales --reference-language-only false",
        "prepare": "husky",
        "prettier": "prettier --check src",
        "prettier:fix": "prettier --write src",
        "release": "standard-version",
        "release:alpha": "standard-version --prerelease alpha",
        "release:beta": "standard-version --prerelease beta",
        "release:rc": "standard-version --prerelease rc",
        "start": "expo start",
        "storybook:generate": "sb-rn-get-stories",
        "storybook:mobile": "cross-env EXPO_PUBLIC_TEST_MODE=true expo start",
        "storybook:web": "cross-env EXPO_PUBLIC_TEST_MODE=true storybook dev -p 7777 -c ./.storybook/storybook-web",
        "test": "jest --no-cache",
        "test:coverage": "jest --coverage",
        "test:coverage:watch": "jest --coverage --watch",
        "test:watch": "jest --watch",
        "web": "expo start --web",
        "web:generated": "concurrently \"yarn build:web\" \"npx serve dist --single\"",
        "generate:graphql": "graphql-codegen --config codegen.ts"
    },
