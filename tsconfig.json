{
  "compilerOptions": {
    "forceConsistentCasingInFileNames": true,
    "paths": {
      "@/*": [
        "./src/*"
      ]
    },
    "strict": true
  },
  "extends": "expo/tsconfig.base",
  "exclude": [
    "src/types/business-api.ts",
    "node_modules"
  ],
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ]
}
